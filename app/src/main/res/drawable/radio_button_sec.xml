<!-- res/drawable/radio_button_selector.xml -->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Checked state -->
    <item android:state_checked="true" android:drawable="@drawable/uncheckitem" />
    <!-- Unchecked state -->
    <item android:state_checked="false" android:drawable="@drawable/checkitem" />
</selector>
